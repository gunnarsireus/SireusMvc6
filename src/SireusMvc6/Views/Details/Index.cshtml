
@section SideBar{
    <div class="col-md-12">
        <h4>Arbete med konvertering till Bootstrap pågår</h4>
    </div>
}

<div class="col-md-12">
    <table class="view" cellspacing="0" cellpadding="0" border="0" id="FormView1"
           style="border-collapse: collapse; border-style: None; border-width: 0px;">
        <tr>
            <td colspan="2">
                <div class="buttonbar buttonbar-top">
                    <a href="/Albums/Index/">
                        <img id="FormView1_Image1" src="/css/Images/button-gallery.gif"
                             style="border-width: 0px;" alt="" />
                    </a> &nbsp;&nbsp;&nbsp;&nbsp;
                    @{
                        ViewBag.Caption = ViewBag.list[ViewBag.Page - 1].Caption;
                        ViewBag.First = ViewBag.list[0].PhotoID;
                        ViewBag.Last = ViewBag.list[ViewBag.list.Count - 1].PhotoID;
                        if (ViewBag.Page > 1)
                        {
                            ViewBag.Prev = ViewBag.list[ViewBag.Page - 2].PhotoID;
                        }
                        else
                        {
                            ViewBag.Prev = ViewBag.First;
                        }
                        if (ViewBag.Page < ViewBag.list.Count)
                        {
                            ViewBag.Next = ViewBag.list[ViewBag.Page].PhotoID;
                        }
                        else
                        {
                            ViewBag.Next = ViewBag.Last;
                        }
                    }
                    <a href="/Details/SetDetails/@string.Concat(ViewBag.First + "/", @ViewData["AlbumID"])">
                        <img src="/css/Images/button-first.gif" style="border-width: 0px;" alt="" />
                    </a>
                    <a href="/Details/SetDetails/@string.Concat(ViewBag.Prev + "/", @ViewData["AlbumID"])">
                        <img src="/css/Images/button-prev.gif" style="border-width: 0px;" alt="" />
                    </a>
                    <a href="/Details/SetDetails/@string.Concat(ViewBag.Next + "/", @ViewData["AlbumID"])">
                        <img src="/css/Images/button-next.gif" style="border-width: 0px;" alt="" />
                    </a>
                    <a href="/Details/SetDetails/@string.Concat(ViewBag.Last + "/", @ViewData["AlbumID"])">
                        <img src="/css/Images/button-last.gif" style="border-width: 0px;" alt="" />
                    </a>
                </div>
                <p>
                    @ViewBag.Caption
                </p>
                <table border="0" cellpadding="0" cellspacing="0" class="photo-frame">
                    <tr>
                        <td class="topx--"></td>
                        <td class="top-x-"></td>
                        <td class="top--x"></td>
                    </tr>
                    <tr>
                        <td class="midx--"></td>
                        <td>
                            <img src="/Handler/Index/PhotoID=@ViewData["PhotoID"]/Size=L" class="photo_198"
                                 style="border: 4px solid white" alt='Photo Number @ViewData["PhotoID"]' />
                        </td>
                        <td class="mid--x"></td>
                    </tr>
                    <tr>
                        <td class="botx--"></td>
                        <td class="bot-x-"></td>
                        <td class="bot--x"></td>
                    </tr>
                </table>
                <p>
                    <a href="/Handler/Download/@ViewData["PhotoID"]/L">
                        <img src="/css/Images/button-download.gif" alt="download this photo" />
                    </a>
                </p>
                <div class="buttonbar">
                    <table border="0">
                        <tr>
                            @{
                                ViewBag.Cnt = 0;
                                for (ViewBag.Cnt = 0; ViewBag.Cnt < ViewBag.List.Count; ViewBag.Cnt++)
                                {
                                    if (ViewBag.Cnt != ViewBag.Page - 1)
                                    {
                                        <text>
                                        <td>
                                            <a href="/Details/SetDetails/@string.Concat(ViewBag.List[ViewBag.cnt].PhotoID + "/", @ViewData["AlbumID"])">@Html.Raw(@ViewBag.Cnt + 1)</a>
                                        </td>
                                        </text>
                                    }
                                    else
                                    {
                                        <text>
                                        <td>
                                            <span>@Html.Raw(@ViewBag.Cnt + 1)</span>
                                        </td>
                                        </text>
                                    }
                                }
                            }
                            <td style="padding-left: 210px">
                                <a href="/Albums/Index">
                                    <img id="FormView1_Image2" src="/css/Images/button-gallery.gif"
                                         style="border-width: 0px;" alt="" />
                                </a> &nbsp;&nbsp;&nbsp;&nbsp;
                                <a href="/Details/SetDetails/@string.Concat(ViewBag.First + "/", @ViewData["AlbumID"])">
                                    <img src="/css/Images/button-first.gif" style="border-width: 0px;" alt="" />
                                </a>
                                <a href="/Details/SetDetails/@string.Concat(ViewBag.Prev + "/", @ViewData["AlbumID"])">
                                    <img src="/css/Images/button-prev.gif" style="border-width: 0px;" alt="" />
                                </a>
                                <a href="/Details/SetDetails/@string.Concat(ViewBag.Next + "/", @ViewData["AlbumID"])">
                                    <img src="/css/Images/button-next.gif" style="border-width: 0px;" alt="" />
                                </a>
                                <a href="/Details/SetDetails/@string.Concat(ViewBag.Last + "/", @ViewData["AlbumID"])">
                                    <img src="/css/Images/button-last.gif" style="border-width: 0px;" alt="" />
                                </a>
                            </td>

                        </tr>
                    </table>
                </div>
            </td>
        </tr>
    </table>
</div>
<script src="/js/site.js"></script>
<script type="text/javascript">SetCurrentPageOnHeader("Home");</script>
