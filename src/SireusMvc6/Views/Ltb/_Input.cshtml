<div class="panel panel-primary">
    <div class="panel-heading">
        <h3 class="panel-title">Serviceperiodens&nbsp;längd</h3>
    </div>
    <div class="row">
        <div class="col-md-3 col-sm-3 ">
            <div class="row">
                <div class="col-md-4 col-sm-4">
                </div>
                <div class="col-md-8 col-sm-8 text-danger BorderOutset">Life&nbsp;Time&nbsp;Buy:</div>
            </div>
        </div>
        <div class="col-md-3 col-sm-3 ">
            <div class="row">
                <input class="col-md-5 col-sm-5  text-danger input-sm BorderInset" name="LTBDate" type="text" id="LTBDate" value="@ViewData["LTBDate"].ToString()" onchange="javascript:GetViewStateLTB();" />
            </div>
        </div>
        <div class="col-md-3 col-sm-3">
            <div class="row">
                <div class="col-md-7 col-sm-7 text-danger BorderOutset">End&nbsp;Of&nbsp;Service:</div>
                <input class="col-md-5 col-sm-5 text-danger BorderInset" name="EOSDate" type="text" id="EOSDate" value="@ViewData["EOSDate"].ToString()" onchange="javascript:GetViewStateEOS();" />
            </div>
        </div>
        <div class="col-md-3 col-sm-3">
            <div class="row">
            </div>
        </div>
    </div>
</div>
<div class="panel panel-primary">
