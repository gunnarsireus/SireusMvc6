<script type="text/javascript">
    $(function () {
        $.datepicker.setDefaults($.datepicker.regional["sv"]);
        $("#LTBDate").datepicker({
            changeMonth: true,
            changeYear: true,
            showWeek: true,
            firstDay: 1
        });

        $(function () {
            $.datepicker.setDefaults($.datepicker.regional["sv"]);
